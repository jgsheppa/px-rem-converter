{
  "manifest_version": 2,
  "name": "Px/Rem",
  "author": "James Sheppard",
  "version": "1.1.0",
  "description": "A pixel to rem converter",
  "icons": {
    "24": "./logos/logo-24.png",
    "48": "./logos/logo-48.png",
    "64": "./logos/logo-64.png",
    "128": "./logos/logo-128.png"
  },
  "permissions": ["tabs"],
  "browser_action": {
    "default_popup": "index.html"
  }
}
